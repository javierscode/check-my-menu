{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
        "@client/*": [
            "src/client/*"
        ],
        "@server/*": [
            "src/server/*"
        ],
        "@shared/*": [
          "src/shared/*"
      ],
        "@test/*":[
            "test/*"
        ]
    },
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js",".eslintrc.js"],
  "exclude": ["node_modules"]
}
