{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["$NODE_ENV", "$POSTGRES_USER", "$POSTGRES_PASSWORD", "$POSTGRES_DB"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
